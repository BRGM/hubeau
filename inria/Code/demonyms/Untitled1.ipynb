    "def solution(A):\n",
    "    A.sort(reverse=True)\n",
    "    n = len(A)\n",
    "    for i in range(0, n-2):\n",
    "        \n",
    "        if A[i] < A[i+1] + A[i+2]:\n",
    "            return A[i] + A[i+1] + A[i+2]\n",
    "            \n",
    "       \n",
    "    return -1"
   ]
